<h1>Vol</h1>
<br>
	<form id='volForm' style='font-size: 15pt;' method="post">
		<input name='name' type='textbox' placeholder='Nom' />
		<input name='author' type='textbox' placeholder='Auteur (du crime)' />
		<button id="search" type="submit" >Go</button>
	</form>
<br>
<div style="width:50%;height:500px;overflow: auto;float:left;">
<ul >
{{#each data}}
<br>
<hr size=3px>
<li>Nom :{{nom}}</li>
<li>Auteur :{{auteur}}</li>
<li>Date :{{date}}</li>
<hr size=3px>
{{/each}}
</ul>
</div>
<div style="width:50%;height:500px;overflow: auto; float:right;">
{{json}}
</div>